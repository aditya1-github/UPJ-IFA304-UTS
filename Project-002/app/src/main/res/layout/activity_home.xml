<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <ImageButton
        android:id="@+id/imageButtonMenuAnggota"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="112dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/menu_anggota" />

    <ImageButton
        android:id="@+id/imageButtonMenuOrganisasi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="112dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButtonMenuAnggota"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/menu_organisasi" />

    <ImageButton
        android:id="@+id/imageButtonMenuBreed"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginTop="64dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonMenuAnggota"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/menu_breed" />

    <ImageButton
        android:id="@+id/imageButtonMenuAcara"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginTop="64dp"
        android:layout_marginEnd="24dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageButtonMenuBreed"
        app:layout_constraintTop_toBottomOf="@+id/imageButtonMenuOrganisasi"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/menu_acara" />

</androidx.constraintlayout.widget.ConstraintLayout>